{
  "name": "@elysiajs/jwt",
  "description": "Plugin for Elysia for using JWT Authentication",
  "version": "0.5.0",
  "author": {
    "name": "saltyAom",
    "url": "https://github.com/SaltyAom",
    "email": "saltyaom@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elysiajs/elysia-jwt"
  },
  "main": "./dist/index.js",
  "exports": {
    "node": "./dist/cjs/index.js",
    "require": "./dist/cjs/index.js",
    "import": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "bugs": "https://github.com/elysiajs/elysia-jwt/issues",
  "homepage": "https://github.com/elysiajs/elysia-jwt",
  "keywords": [
    "elysia",
    "jwt",
    "auth",
    "authentication"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "bun run --hot example/index.ts",
    "test": "bun wiptest && npm run test:node",
    "test:node": "npm install --prefix ./test/node/cjs/ && npm install --prefix ./test/node/esm/ && node ./test/node/cjs/index.js && node ./test/node/esm/index.js",
    "build": "rimraf dist && tsc --project tsconfig.esm.json && tsc --project tsconfig.cjs.json",
    "release": "npm run build && npm run test && npm publish --access public"
  },
  "dependencies": {
    "jose": "^4.14.4"
  },
  "devDependencies": {
    "@elysiajs/cookie": "^0.3.0",
    "@sinclair/typebox": "^0.28.10",
    "@types/node": "^20.1.4",
    "bun-types": "^0.5.8",
    "elysia": "0.5.12",
    "eslint": "^8.40.0",
    "rimraf": "4.3",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "elysia": ">= 0.5.12"
  }
}